<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <PublishProvider>AzureWebSite</PublishProvider>
    <LastUsedBuildConfiguration />
    <LastUsedPlatform />
    <SiteUrlToLaunchAfterPublish>http://propertytycoon.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>propertytycoon.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>PropertyTycoon</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$PropertyTycoon</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DefaultConnection" Order="1" Enabled="False">
          <Destination Path="Data Source=tcp:ptycoondb.database.windows.net,1433;Initial Catalog=PropertyTycoonDB;User ID=pdhar@ptycoondb;Password=Test@123" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="PropertyTycoon.Models.ApplicationDbContext, PropertyTycoon" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="DataLayer.GameContext" Order="2" Enabled="True">
          <Destination Path="Data Source=tcp:ptycoondb.database.windows.net,1433;Initial Catalog=PropertyTycoonDB;User ID=pdhar@ptycoondb;Password=Test@123" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="DataLayer.GameContext, DataLayer" MigrationConfiguration="DataLayer.Migrations.Configuration, DataLayer" Origin="Convention" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DataLayer.GameContext-Web.config Connection String">
      <ParameterValue>Data Source=tcp:ptycoondb.database.windows.net,1433;Initial Catalog=PropertyTycoonDB;User ID=pdhar@ptycoondb;Password=Test@123</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DefaultConnection-Web.config Connection String">
      <ParameterValue>Data Source=tcp:ptycoondb.database.windows.net,1433;Initial Catalog=PropertyTycoonDB;User ID=pdhar@ptycoondb;Password=Test@123</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
  <ItemGroup>
    <_ConnectionStringsToInsert Include="DataLayer.GameContext" />
  </ItemGroup>
</Project>